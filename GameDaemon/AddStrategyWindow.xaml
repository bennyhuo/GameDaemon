<Window x:Class="GameDaemon.AddStrategyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        Title="添加策略" Height="244.776" Width="307.462" ResizeMode="NoResize" FocusManager.FocusedElement="{Binding ElementName=starttime}" KeyDown="onKeyDown" Icon="res/panda.ico">
    <Grid>
        
        <Button Name="CancelButton" TabIndex="3" Content="取消" HorizontalAlignment="Left" Margin="155,145,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.6,-1.359" Click="cancel"/>
        <xctk:TimePicker Name="starttime" TabIndex="0" HorizontalAlignment="Left" Height="23" Margin="123,39,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Name="ConfirmButton" TabIndex="2" Content="确定" HorizontalAlignment="Left" Margin="55,145,0,0" VerticalAlignment="Top" Width="75" Click="confirm"/>
        <xctk:TimePicker Name="endtime" TabIndex="1" HorizontalAlignment="Left" Height="23" Margin="123,86,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="起始时间" HorizontalAlignment="Left" Margin="55,39,0,0" VerticalAlignment="Top"/>
        <Label Content="结束时间" HorizontalAlignment="Left" Margin="55,83,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
